{
    "window.zoomLevel": 1,
    "files.associations": {
        "*.wxml": "html",
        "*.wxss": "css"
    },
    "eslint.enable": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
        "html"
    ]
}